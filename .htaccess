RewriteEngine on

RewriteRule ^/static/config.js /static/config.js.php

RewriteRule ^service$ /ajax.php
RewriteRule ^login$ /login.php
RewriteRule ^register$ /register.php
RewriteRule ^auth$ /auth-grant.php
RewriteRule ^api$ /auth.php
RewriteRule ^lostpass$ /lostpass.php
RewriteRule ^resetpwd$ /resetpwd.php
RewriteRule ^ucenter$ /ucenter.php
RewriteRule ^verify$ /verify.php
RewriteRule ^help$ /help.php

ErrorDocument 404 /404.php
